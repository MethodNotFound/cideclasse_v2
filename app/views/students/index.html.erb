<div id="students" class="mt-3">
  <% @students.each do |student| %>
    <div class="flex justify-between w-full pb-3">
      Nome: <%= student.name %>
      <%= link_to "Mostrar cadastro", student, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium text-black" %>
    </div>
  <% end %>
</div>
